<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
    <style>
        table {
            width: 60%;
            border-collapse: collapse;
            margin: 20px auto;
            font-family: Arial, sans-serif;
        }

        th, td {
            padding: 10px;
            border: 1px solid #444;
            text-align: left;
        }

        th {
            background: #f2f2f2;
        }

        tr:nth-child(even) {
            background: #fafafa;
        }
    </style>
</head>
<body>
   <table>
        <tr>
            <th>Id</th>
            <th>Email</th>
            <th>username</th>
            <th>Edit</th>
            <th>Add New</th>
        </tr>

        <% for (let user of users) { %>
        <tr>
            <td><%= user.id %></td>
            <td><%= user.email %></td>
            <td><%= user.username %></td>
            <td><form action="/user/<%=user.id %>/edit" method="get">
                <button>Edit username</button>

            </form></td>
            <td><form action="/user/new" method="get">
                <button>ADD</button>

            </form></td>
        </tr>
        <% } %>
    </table>
</body>
</html>

